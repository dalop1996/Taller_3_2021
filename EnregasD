package Dominio;

public class EntregasD extends Entregas{
	private double peso;
	private double grosor;
	private double valor;
	
	public EntregasD(String codigo, String tipo, String remitente, String destinatario, double peso,
			double grosor, double valor) {
		super(codigo, tipo, remitente, destinatario);
		this.peso = peso;
		this.grosor = grosor;
		this.valor = valor;
	}

	public double getPeso() {
		return peso;
	}

	public void setPeso(double peso) {
		this.peso = peso;
	}

	public double getGrosor() {
		return grosor;
	}

	public void setGrosor(double grosor) {
		this.grosor = grosor;
	}

	public double getValor() {
		return valor;
	}

	public void setValor(double valor) {
		this.valor = valor;
	}
	public String toString() {
        return super.toString()+" Peso: " + peso + " grosor: " + grosor + " valor:" + valor;
    }
	
}
