package Dominio;

public class EntregasE extends Entregas{
	private double peso;
	private double largo;
	private double ancho;
	private double profundidad;
	private double valor;
	public EntregasE(String codigo, String tipo, String remitente, String destinatario, double peso,
			double largo, double ancho, double profundidad, double valor) {
		super(codigo, tipo, remitente, destinatario);
		this.peso = peso;
		this.largo = largo;
		this.ancho = ancho;
		this.profundidad = profundidad;
		this.valor = valor;
	}
	public double getPeso() {
		return peso;
	}
	public void setPeso(double peso) {
		this.peso = peso;
	}
	public double getLargo() {
		return largo;
	}
	public void setLargo(double largo) {
		this.largo = largo;
	}
	public double getAncho() {
		return ancho;
	}
	public void setAncho(double ancho) {
		this.ancho = ancho;
	}
	public double getProfundidad() {
		return profundidad;
	}
	public void setProfundidad(double profundidad) {
		this.profundidad = profundidad;
	}
	public double getValor() {
		return valor;
	}
	public void setValor(double valor) {
		this.valor = valor;
	}
	public String toString() {
        return super.toString()+" Peso: " + peso + " largo: " + largo + " ancho: " + ancho + " profundidad: " + profundidad
                + " valor: " + valor ;
    }
	
}
